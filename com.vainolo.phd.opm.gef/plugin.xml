<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.vainolo.phd.opm.gef.editor.OPMGraphicalEditor"
            contributorClass="com.vainolo.phd.opm.gef.editor.OPMGraphicalEditorActionBarContributor"
            default="true"
            extensions="opm"
            id="com.vainolo.phd.opm.gef.editorID"
            name="OPM GEF Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.vainolo.phd.opm.gef.OPLViewPart"
            id="com.vainolo.phd.opm.gef.oplView"
            name="OPL"
            restorable="true">
      </view>
   </extension>

</plugin>
